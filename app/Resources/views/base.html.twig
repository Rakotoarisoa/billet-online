<!DOCTYPE html>
<html lang="fr">
<head>
    <meta charset="UTF-8"/>
    <meta name="description" content="">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
    <title>{% block title %}Evènements{% endblock %}</title>
    {% block stylesheets %}
        <!-- Stylesheet -->
        <link rel="stylesheet" href="{{ asset('build/css/app.css') }}">
        <link rel="stylesheet" href="{{ asset('css/style.css') }}">
    {% endblock %}
    {% block head_javasripts %}
    {% endblock %}
    <link rel="icon" type="image/x-icon" href="{{ asset('favicon.ico') }}"/>
</head>
<body>
<!-- ##### Header Area Start ##### -->
{% block header %}
    <div class="preloader d-flex align-items-center justify-content-center">
        <div class="lds-ellipsis">
            <div></div>
            <div></div>
            <div></div>
            <div></div>
        </div>
    </div>
    <header class="header-area">
        <!-- Navbar Area -->
        <div class="oneMusic-main-menu">
            <div class="classy-nav-container breakpoint-off">
                <div class="container">
                    <!-- Menu -->
                    <nav class="classy-navbar justify-content-between" id="oneMusicNav">
                        <!-- Nav brand -->
                        <a href="" class="nav-brand"><img width="156" heigth="17" src="{{ asset('img/ivenco.png') }}" alt=""></a>
                        <!-- Navbar Toggler -->
                        <div class="classy-navbar-toggler">
                            <span class="navbarToggler"><span></span><span></span><span></span></span>
                        </div>
                        <!-- Menu -->
                        <div class="classy-menu">
                            <!-- Close Button -->
                            <div class="classycloseIcon">
                                <div class="cross-wrap"><span class="top"></span><span class="bottom"></span></div>
                            </div>
                            <!-- Nav Start -->
                            <div class="classynav">
                                <ul>
                                    <li><a href="{{ path('viewList') }}">Acceuil</a></li>
                                    {% if is_granted("IS_AUTHENTICATED_REMEMBERED") %}
                                        <li><a href="{{ path('createEvent',{'userId':app.user.id }) }}">Créer un
                                                évènement</a></li>
                                        <li><a href="{{ path('viewListUser',{'userId':app.user.id}) }}">Gérer les
                                                évènements</a></li>
                                    {% endif %}
                                    <li><a href="{{ path('support') }}">Aide</a></li>
                                    <li><a href="{{ path('contact') }}">Contact</a></li>
                                </ul>
                                <!-- Login/Register & Cart Button -->
                                <div class="login-register-cart-button d-flex align-items-center">
                                    <!-- Login/Register -->
                                    <div class="login-register-btn mr-50">
                                        {% if is_granted("IS_AUTHENTICATED_REMEMBERED") %}
                                            <span style="color: #FFFFFF;"><a style="font-size: 15px;font-weight: inherit;" href="{{ path('fos_user_profile_show') }}">{{ 'layout.logged_in_as'|trans({'%username%': app.user.username}, 'FOSUserBundle') }}</a></span> |
                                            <a href="{{ path('fos_user_security_logout') }}">
                                                {{ 'layout.logout'|trans({}, 'FOSUserBundle') }}
                                            </a>
                                        {% else %}
                                            <a id="loginBtn" data-toggle="tooltip" title="Vous pouvez créer et administrer des évènements en étant connecté" data-placement="bottom"
                                               href="{{ path('fos_user_security_login') }}">{{ 'layout.login'|trans({}, 'FOSUserBundle') }}
                                            </a> <b style="color: #FFF;">|</b>
                                            <a id="register"
                                               href="{{ path('fos_user_registration_register') }}">{{ 'layout.register'|trans({}, 'FOSUserBundle') }}
                                            </a>
                                        {% endif %}
                                    </div>
                                    <!-- Cart Button -->
                                    <div class="cart-btn">
                                        <p><span class="fa fa-ticket"></span> <a data-toggle="tooltip" data-placement="bottom" title="Vous avez {{ not app.session.get('quantity') ? "0" : app.session.get('quantity') }} sortes de billets à commander" href="{{ path('cart_index') }}"><span
                                                        class="quantity">{{ not app.session.get('quantity') ? "0" : app.session.get('quantity') }}</span></a>
                                        </p>
                                    </div>
                                </div>
                            </div>
                            <!-- Nav End -->
                        </div>
                    </nav>
                </div>
            </div>
        </div>
    </header>
{% endblock %}
<!-- ##### Header Area End ##### -->
<!-- ##### Breadcumb Area Start ##### -->
